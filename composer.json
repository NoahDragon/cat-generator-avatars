  "keywords": [
    "avatar",
    "image",
    "profile",
    "user"
  ],
  "homepage": "https://github.com/NoahDragon/cat-generator-avatars",
  "license": "MIT",
  "authors": [
    {
      "name": "Abner Chou",
      "email": "hi@abnerchou.me",
      "homepage": "http://abnerchou.me",
      "role": "Developer"
    }
  ],
  "support": {
